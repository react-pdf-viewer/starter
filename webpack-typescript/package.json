{
    "name": "react-pdf-viewer-webpack-typescript",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "copy": "cp ../assets/pdf-open-parameters.pdf dist",
        "typecheck": "tsc --noEmit",
        "build": "rm -rf dist && webpack --mode production && npm run copy",
        "dev": "rm -rf dist && mkdir dist && npm run copy && NODE_ENV=development webpack serve"
    },
    "dependencies": {
        "@react-pdf-viewer/core": "2.5.0",
        "@react-pdf-viewer/default-layout": "2.5.0",
        "pdfjs-dist": "2.7.570",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "babel-loader": "^8.2.2",
        "css-loader": "^5.2.1",
        "html-webpack-plugin": "^5.3.1",
        "mini-css-extract-plugin": "^1.4.1",
        "ts-loader": "^8.0.14",
        "typescript": "^4.2.4",
        "webpack": "^5.23.0",
        "webpack-cli": "^4.6.0",
        "webpack-dev-server": "^3.11.2"
    }
}
